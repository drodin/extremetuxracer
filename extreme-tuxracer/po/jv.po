msgid "Tux"
msgstr "Tux"

#: po/modelinfo:2
msgid "Samuel"
msgstr "Samuel"

#: po/modelinfo:3
msgid "Trixi"
msgstr "Trixi"

#: po/modelinfo:4
msgid "Speedy"
msgstr "Speedy"
